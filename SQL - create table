CREATE TABLE `test`.`sensor` (
`id` INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
`time` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
`h` VARCHAR( 10 ) NOT NULL,`t` VARCHAR( 10 ) NOT NULL,
`f` VARCHAR( 10 ) NOT NULL,
`h2` VARCHAR( 10 ) NOT NULL, `t2` VARCHAR( 10 ) NOT NULL,  `f2` VARCHAR( 10 ) NOT NULL, `r1` VARCHAR( 10 ) NOT NULL, `r2` VARCHAR( 10 ) NOT NULL
)
